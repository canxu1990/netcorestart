{
  "SiteBaseConfig": {
    //数据库Mysql
    "DataBase": "mysql",
    //文件上传路径
    "FileUpPath": "/upload/",
    //是否启用单用户登录
    "IsSingleLogin": "True",
    //允许上传的文件格式
    "AttachExtension": "gif,jpg,jpeg,png,bmp,rar,zip,doc,docx,xls,xlsx,ppt,pptx,txt,flv,apk,mp4,mpg,ts,mpeg,mp3,bak,pdf",
    //图片上传最大值KB
    "AttachImagesize": 12400,
    //是否使用Redis
    "UserRedis": false,
    //Redis连接
    "Redis_ConnectionString": "127.0.0.1:6379,localhost:6379",
    //Redis实例名称
    "Redis_InstanceName": "Test"

  }

}
